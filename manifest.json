{
 "manifest_version":2,
 
 "name":"多搜",
 "description":"This extension enables you to use multiple search engines.",
 "version":"1.0",
 
 "browser_action":{
  "default_icon": "icon.png",
  "default_popup": "popup.html",
  "default_title": "多搜"
 },

 "permissions":[
  "tabs",
  "activeTab",
  "contextMenus"
 ],

 "background":{
  "scripts": ["background.js"]
 }
}

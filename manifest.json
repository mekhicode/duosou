{
 "manifest_version":2,
 
 "name":"多搜",
 "description":"This extension enables you to use multiple search engines.",
 "version":"1.0",

 "icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
 },
 
 "browser_action":{
  "default_icon": "icon.png",
  "default_popup": "popup.html",
  "default_title": "多搜"
 },

 "permissions":[
  "tabs",
  "activeTab",
  "contextMenus",
  "storage"
 ],

 "background":{
  "scripts": ["background.js"]
 }
}
